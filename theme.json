{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "layout": {
      "contentSize": "calc(min(100% - 192px, 1920px))",
      "wideSize": "1920px",
      "allowCustomContentAndWideSize": false
    },
    "blocks": {
      "core/column": {
        "shadow": {
          "defaultPresets": false,
          "presets": []
        }
      },
      "core/button": {
        "border": {
          "radius": false,
          "style": false,
          "color": false,
          "width": false
        },
        "shadow": {
          "defaultPresets": false,
          "presets": []
        }
      },
      "core/group": {
        "layout": {
          "contentSize": "var(--wp--style--global--content-size)",
          "wideSize": "var(--wp--style--global--wide-size)",
          "allowCustomContentAndWideSize": false
        }
      },
      "core/row": {
        "layout": {
          "contentSize": "var(--wp--style--global--content-size)",
          "wideSize": "var(--wp--style--global--wide-size)"
        }
      },
      "core/pullquote": {
        "border": {
          "radius": false,
          "style": false,
          "color": true,
          "width": false
        }
      },
      "core/image": {
        "layout": {
          "contentSize": "var(--wp--style--global--content-size)",
          "wideSize": "var(--wp--style--global--wide-size)",
          "allowCustomContentAndWideSize": false
        },
        "shadow": {
          "defaultPresets": false,
          "presets": []
        }
      },
      "core/gallery": {
        "layout": {
          "contentSize": "var(--wp--style--global--content-size)",
          "wideSize": "var(--wp--style--global--wide-size)"
        }
      }
    },
    "typography": {
      "fontWeight": false,
      "fontStyle": false,
      "dropCap": false,
      "customFontSize": false,
      "letterSpacing": false,
      "fontFamilies": [
        {
          "fontFamily": "Synthese, sans-serif",
          "slug": "Synthese"
        },
        {
          "fontFamily": "CabinetGrotesk-Black, sans-serif",
          "slug": "CabinetGrotesk Black"
        },
        {
          "fontFamily": "CabinetGrotesk-Bold, sans-serif",
          "slug": "CabinetGrotesk Bold"
        }
      ],
      "fontSizes": [
        {
          "name": "Heading 1",
          "slug": "head-1",
          "size": "90px"
        },
        {
          "name": "Heading 2",
          "slug": "head-2",
          "size": "56px"
        },
        {
          "name": "Heading 3",
          "slug": "head-3",
          "size": "40px"
        },
        {
          "name": "Heading 4",
          "slug": "head-4",
          "size": "24px"
        },
        {
          "name": "Paragraph",
          "slug": "paragraph",
          "size": "18px"
        },
        {
          "name": "Subtitle",
          "slug": "subtitle",
          "size": "38px"
        },
        {
          "name": "Small",
          "slug": "small",
          "size": "12px"
        }
      ]
    },
    "color": {
      "link": true,
      "palette": [
        {
          "name": "Groovy Black",
          "slug": "text",
          "color": "#1A1616"
        },
        {
          "name": "Groovy Blue",
          "slug": "primary",
          "color": "#D7F4FF"
        },
        {
          "name": "Groovy Green",
          "slug": "secondary",
          "color": "#D3FFAC"
        },
        {
          "name": "Groovy Orange",
          "slug": "tertiary",
          "color": "#FFE5CE"
        },
        {
          "name": "Groovy Yellow",
          "slug": "smooth-grey",
          "color": "#F2FFA3"
        },
        {
          "name": "Groovy Pink",
          "slug": "mid-grey",
          "color": "#F8D7FE"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#FFFFFF"
        },
        {
          "name": "Transparent",
          "slug": "transparent",
          "color": "#00000000"
        }
      ],
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false 
    }
  }
}